  "query": "\n            SELECT id, recipient_did, actor_did, kind, subject_uri, reference_uri,\n                   read, created_at as \"created_at: DateTime<Utc>\"\n            FROM notifications\n            WHERE recipient_did = $1 AND id < $2\n            ORDER BY id DESC\n            LIMIT $3\n            ",
